{
  "customFunctions": [],
  "importedLibraries": [],
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  
  "MicrosoftAppId": "",
  "publishTargets": [],
  "runtime": {
    "command": "func start --script-root bin\\Debug\\netcoreapp3.1",
    "customRuntime": true,
    "key": "adaptive-runtime-dotnet-functions",
    "path": "../"
  },
  "runtimeSettings": {
    "adapters": [
      {
        "enabled": true,
        "name": "activities",
        "route": "activities",
        "type": "Microsoft.Bot.Builder.Dialogs.Adaptive.Runtime.CoreBotAdapter"
      }
    ],
    "features": {
      "removeRecipientMentions": false,
      "showTyping": false,
      "traceTranscript": false,
      "useInspection": false,
      "setSpeak": {
        "voiceFontName": "en-US-JennyNeural",
        "fallbackToTextForSpeechIfEmpty": true
      }
    },
    "components": [],
    "skills": {
      "allowedCallers": []
    },
    "storage": "",
    "telemetry": {
      "logActivities": true,
      "logPersonalInformation": false,
      "options": {
        "connectionString": ""
      }
    }
  },
  "skillConfiguration": {}
}